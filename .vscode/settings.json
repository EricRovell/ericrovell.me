{
	"files.associations": {
		"*.mdx": "markdown"
	},
	"eslint.validate": [
		"javascript",
		"typescript",
		"svelte"
	],
	"cssVariables.lookupFiles": [
		"src/**/tokens.css",
		"src/**/theme-*.css"
	],
	"search.exclude": {
		"**/.svelte-kit": true,
		"**/build": true
	},
	"cSpell.language": "en,ru,en-US,ru-RU"
}
